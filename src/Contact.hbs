{{#> base title="Contact Us" currentPath="/Contact" }}

{{#*inline "page-head"}}

    <link rel="stylesheet" href="modals.css?v={{ version }}">

{{/inline}}

{{#*inline "main-block"}}

    <div id="heroSpace">
        {{> picture id="heroImg" title="Move Removals" alt="A smart looking van moves efficiently from left to right" file="/images/van" ext="jpg" type="jpeg" }}

        <div id="heroBtnWrapperOuter">
            <div id="heroBtnWrapperInner">
                <button id="heroBtn" class="modal-btn" data-modal-target="#requestCallback">Request A Callback</button>
            </div>
        </div>
    </div>

    <article class="w-100 mb-auto">

        <h2 class="fc-primary">{{> tbo }} Contact Us {{> tbc }}</h2>

        <p class="mb-1">
            For all enquiries
            <br>
            Call: <a class="px-1" href="tel:01613303300">0161 330 3300</a>
            <br>
            Email <a class="px-1" href="mailto:enquiries@moveremovals.co.uk"> enquiries@moveremovals.co.uk</a>
            <br>
            Or complete the form below for a free, no obligation, quote.
        </p>

        <iframe src="https://docs.google.com/forms/d/e/1FAIpQLSerxT6JvezhgzFHgOw-0ModTjtbOyWp0u9YdqfRrgKg_CsMeA/viewform?embedded=true" width="640" height="2400" frameborder="0" marginheight="0" marginwidth="0">Loading…</iframe>
    </article>

{{/inline}}

{{#*inline "body-end"}}

    <div id="requestCallback" class="modal">
        <span class="modal-closer" data-modal-target="#requestCallback">X</span>
            <iframe src="https://docs.google.com/forms/d/e/1FAIpQLSejyGhRnPQ6eVZ0bbyaGxPUKvSpqjLK1jKtzuouqEuC-zcNdA/viewform?embedded=true" width="640" height="700" frameborder="0" marginheight="0" marginwidth="0">Loading…</iframe>
        </div>

    <script src="scripts/modals.js"></script>

{{/inline}}

{{/base}}